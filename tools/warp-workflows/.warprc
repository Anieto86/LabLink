# Source the LabLink commands
if [ -f "/c/Users/alvar/Documents/LabLink-node/warp-workflows/warp-commands.sh" ]; then
    source "/c/Users/alvar/Documents/LabLink-node/warp-workflows/warp-commands.sh"
fi

# Custom prompt for LabLink sessions
lablink_prompt() {
    local current_dir=$(pwd)
    if [[ "$current_dir" == *"LabLink"* ]]; then
        echo "üß¨ LabLink"
    fi
}

# Auto-complete para comandos LabLink
_lablink_complete() {
    local cur="${COMP_WORDS[COMP_CWORD]}"
    local commands="ll-sync ll-study ll-status ll-quick lablink_full lablink_check lablink_reset lablink_session"
    COMPREPLY=($(compgen -W "$commands" -- "$cur"))
}

complete -F _lablink_complete lablink
